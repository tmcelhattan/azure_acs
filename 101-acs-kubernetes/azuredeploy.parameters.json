{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "dnsNamePrefix": {
      "value": "roadmapapp"
    },
    "agentCount": {
      "value": 1
    },
    "agentVMSize": {
      "value": "Standard_A2"
    },
    "linuxAdminUsername": {
      "value": "azureuser"
    },
    "orchestratorType": {
      "value": "Kubernetes"
    },
    "masterCount": {
      "value": 1
    },
    "sshRSAPublicKey": {
      "value": "<ssh_pub_key>"
    },
    "servicePrincipalClientId": {
      "value": "<sp_principal_id>"
    },
    "servicePrincipalClientSecret": {
      "value": "<sp_principal_password>"
    }
  }
}